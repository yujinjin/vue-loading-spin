{
    "name": "vue-loading-spin",
    "version": "1.0.1",
    "description": "'vue-loading-spin'是一个基于VUE实现的加载提示组件，也可以作为组件。",
    "main": "dist/loading.js",
    "unpkg": "dist/loading.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/yujinjin/vue-loading-spin.git"
    },
    "scripts": {
        "demo": "vue-cli-service serve --mode demo --copy",
        "build": "vue-cli-service build --mode demo",
        "lib": "vue-cli-service build --mode lib --target lib --inline-vue --name vue-loading-spin src/index.js",
        "lint": "vue-cli-service lint"
    },
    "keywords": [
        "vue",
        "loading"
    ],
    "author": "jinyu <yujinjin9@126.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/yujinjin/vue-loading-spin/issues"
    },
    "dependencies": {
        "vue": "^2.6.14"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.13",
        "@vue/cli-plugin-eslint": "~4.5.13",
        "@vue/cli-plugin-router": "~4.5.13",
        "@vue/cli-plugin-unit-jest": "~4.5.13",
        "@vue/cli-plugin-vuex": "~4.5.13",
        "@vue/cli-service": "~4.5.13",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/test-utils": "^1.2.2",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.32.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^7.18.0",
        "less": "^3.0.4",
        "less-loader": "^5.0.0",
        "node-sass": "^4.14.1",
        "sass-loader": "^9.0.3",
        "prettier": "^2.4.1",
        "vue-template-compiler": "^2.6.14",
        "babel-plugin-import": "^1.13.3"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "prettier": {
        "tabWidth": 4,
        "useTabs": false,
        "jsxBracketSameLine": false,
        "printWidth": 1000,
        "proseWrap": "never",
        "trailingComma": "none",
        "arrowParens": "avoid",
        "semi": true,
        "singleQuote": false
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
